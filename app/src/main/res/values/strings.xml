    <string name="all_items_deleted">Deleted</string>

    <!-- Label on a button that the user clicks to undo. -->
    <string name="undo">UNDO</string>

    <!-- Label on the welcome notification. -->
    <string name="welcome">Welcome to Notification History</string>

    <!-- Message on the welcome notification. -->
    <string name="setup">Thank you for downloading Notification History.\n
        To record notifications, enable "Notification History" in "Notification access" settings.</string>

    <!-- Label on a button that the user clicks to open notification access settings. -->
    <string name="click_to_enable">click to enable</string>

    <!-- Message that appears on the background when there is notifications. -->
