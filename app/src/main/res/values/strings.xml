<resources>
    <!-- Name of the application, shown in the application's title bar. -->
    <string name="app_name">Notification History</string>

    <!-- Shown as the title of the notification history. -->
    <string name="title_activity_main">Notification History</string>

    <!-- Shown as the title of the settings. -->
    <string name="title_activity_settings">Settings</string>

    <!-- Shown as the title of the exclude applications settings. -->
    <string name="title_activity_exclude_settings">Exclude Applications</string>

    <!-- Menu item that brings up the application's Settings screen. -->
    <string name="settings">Settings</string>

    <!-- Menu item that allows the user to clear all notifications. -->
    <string name="clear_all">Clear all</string>

    <!-- Menu item that allows the developer to issue notification for debugging. -->
    <string name="issue_notification">notify</string>

    <!-- Menu item that allows the developer to issue notification for debugging. -->
    <string name="issue_notification_with_no_action">notify 2</string>

    <!-- Menu item that allows the developer to open notification access settings. -->
    <string name="notification_access">Notification access</string>

    <!-- Message that appears as feedback when the user has indicated notification item which has no action. -->
    <string name="no_action_associated">No action associated</string>

